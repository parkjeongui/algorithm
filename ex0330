class Solution {

    public long solution(int a, int b) {

        long answer = 0;

            if(a<b){
            	
                for(int i=a; a<=b;a++)
                    answer += a;
            }
            else{
                for(int i=a; a>=b;a--)
                    answer += a;
            
          }
        return answer;
    }
    public static void main(String args[]){
        int a = 3, b= 5;

        Solution solution = new Solution();
        System.out.println(solution.solution(a,b));
    }
    
  }
